%class "ParserLogic";

%terminals AND, OR, NEGATION, TRUE, FALSE, LPARENTH, RPARENTH, VARIABLE, NEWLINE;

%goal S;

S = S L
    | L
    ;

L = Expr NEWLINE
    ;

Expr = Expr AND Term
    | Term
    ;

Term = Term OR Factor
    | Factor
    ;

Factor = TRUE
    | FALSE
    | VARIABLE
    | LPARENTH Expr RPARENTH
    | NEGATION LPARENTH Expr RPARENTH
    ;
